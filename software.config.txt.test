#Config file
# True comments with "#"
#Program name code by $NAME
#under the program name (empty lines do not count), the option are symbolized by key=value (ex -n=5) or key alone (ex m)

#first program config
$cutadapt
-e=0.1
-O=10
-m=35
-q=20
--overlap=7

#2nd program config
$samtools view single
-h
-b
-F=0x04

$samtoolsView
-h
-F=0x02
-S



#3nd program config
$bwa Aln 1
-n=5
-o=1
-e=-1

$bwaIndex
-a=is

#4th program
$GATK UnifiedGenotyper
-T=UnifiedGenotyper
-glm=BOTH
-stand_call_conf=50
-stand_emit_conf=10
-dcov=200
--output_mode=EMIT_ALL_CONFIDENT_SITES

$GATK gatkVariantFiltration
-T=VariantFiltration

$GATK gatkSelectVariants
-T=SelectVariants
#-selectType=SNP

$GATK gatkReadBackedPhasing
-T=ReadBackedPhasing
 
$GATK gatkBaseRecalibrator
-T=BaseRecalibrator

$GATK gatkRealignerTargetCreator
-T=RealignerTargetCreator
--fix_misencoded_quality_scores -fixMisencodedQual

$GATK gatkIndelRealigner
-T=IndelRealigner

$gatkHaplotypeCaller
-T=HaplotypeCaller


$picardToolsSortSam
SORT_ORDER=coordinate
VALIDATION_STRINGENCY=SILENT

$cleaner
1
2
3
4



$order
1=bwaAln 1
2=bwaSampe

